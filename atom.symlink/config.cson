"*":
  "atom-beautify":
    general:
      beautifyEntireFileOnSave: false
    html:
      wrap_line_length: 80
  "atom-material-ui":
    colors:
      abaseColor: "#607d8b"
      accentColor: "#b27252"
      genAccent: true
      predefinedColor: "Blue Grey"
    fonts:
      fontSize: 15
    tabs:
      tintedTabBar: true
    treeView:
      blendTabs: true
      compactList: true
    ui:
      panelContrast: true
      panelShadows: true
  "autocomplete-modules":
    babelPluginModuleResolver: true
    webpack: true
  core:
    disabledPackages: [
      "background-tips"
      "exception-reporting"
      "metrics"
    ]
    followSymlinks: false
    hideGitIgnoredFiles: true
    ignoredNames: [
      ".bundle"
      ".git"
      "log"
      "repositories"
      "tmp"
      "vendor"
    ]
    telemetryConsent: "no"
    themes: [
      "atom-material-ui"
      "atom-material-syntax"
    ]
  editor:
    fontFamily: "Operator Mono"
    invisibles: {}
    showIndentGuide: true
    showInvisibles: true
    softWrapAtPreferredLineLength: true
  "go-plus":
    panelDisplayMode: "closed"
  "linter-eslint":
    lintHtmlFiles: true
    useGlobalEslint: true
  "linter-ui-default": {}
  "release-notes":
    viewedVersion: "0.89.0"
  "spell-check":
    grammars: [
      "text.plain"
      "source.gfm"
      "text.git-commit"
      "text.html.basic"
    ]
  "tree-view": {}
  welcome:
    showOnStartup: false
  zentabs:
    neverCloseUnsaved: true
